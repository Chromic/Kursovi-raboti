#define _CRT_SECURE_NO_WARNINGS
#define _CRT_NONSTDC_NO_WARNINGS
#include "stdafx.h"
#include <stdio.h>
#include <stdlib.h>
#include <math.h>
#include <conio.h>
#include <string.h>
//ONUR

void main()
{
	system("chcp 1251");
	int i, j, k, br, rows, cols;
	int A[100][100];
	int a[100];
	printf(" Въведете брой редове (<=100) \n");
	scanf("%d", &rows);
	printf(" Въведете брой стълбове(<=100) \n");
	scanf("%d", &cols);
	for (i = 0; i < rows; i++)
		for (j = 0; j < cols; j++)
		{
			printf("A[%d][%d]=", i, j);
			scanf("%d", &A[i][j]);
		}
	for (i = 0; i < rows; i++)
	{
		for (br = 0, k = 0, j = 0; j < cols; j++)
		{
				br = A[i][j];
				br = pow(br, 3);
				k = br;
		}
		a[i] = k;
		printf("a[%d]=%d \n", i, a[i]);
	}
	getch();
}
